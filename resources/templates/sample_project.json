    "db": {
        "connection": "mysql",
        "host": "127.0.0.1",
        "port": 3306,
        "database": "laravel",
        "username": "root",
        "password": ""
    },
    "prevent_lazy_loading": false,
    "prevent_silently_discarding_attributes": false,
    "prevent_accessing_missing_attributes": false,
    "session_driver": "file",
    "cache_store": "file",

    "filament": {
        "panel_name": "admin",
