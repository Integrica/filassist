        "notifications" : false,
        "actions" : false,
        "password_forgotten" : false
    },

    "locales": [ 
        "en",
        "de"
    ],

    "packages": [
        {
            "package": "barryvdh/laravel-debugbar",
            "dev": true,
            "disable-views": false
        },
        {
